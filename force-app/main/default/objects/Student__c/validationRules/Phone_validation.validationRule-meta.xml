<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(LEN(Phone__c) &lt;&gt; 10,LEN(Phone__c) &lt;&gt; 12)</errorConditionFormula>
    <errorDisplayField>Phone__c</errorDisplayField>
    <errorMessage>Phone number must be in between 10 or 12</errorMessage>
</ValidationRule>
